CREATE TABLE PROJ
(
 CD_PROJ CHAR(6) NOT NULL,
 NM_PROJ VARCHAR(30) NOT NULL,
 CD_DEPTO CHAR(3) NOT NULL
            REFERENCES DEPTO(CD_DEPTO),
 CD_RESP NUMBER(5) NOT NULL 
            REFERENCES FUNC (CD_MAT),
 QT_EQP NUMBER(02),
 DT_INI DATE,
 DT_FIM DATE,
 PRIMARY KEY (CD_PROJ)
)
STORAGE(initial 4k next 4k minextents 1 maxextents 5)
PCTFREE 10
PCTUSED 80;